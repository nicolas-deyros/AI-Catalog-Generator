{
  "filters": {},
  "rules": {
    "common-misspellings": true,
    "write-good": {
      "passive": false,
      "illusion": false,
      "so": false,
      "thereIs": false,
      "weasel": false,
      "adverb": false,
      "tooWordy": false
    },
    "spellcheck-tech-word": true,
    "terminology": {
