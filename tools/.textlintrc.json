      "terms": [
        "AI Catalog Generator",
        "API key",
        "TypeScript",
        "JavaScript",
        "React",
        "Vite",
        "Tailwind CSS",
        "GitHub",
        "npm",
        "Node.js",
        "PDF",
        "CSS",
        "HTML",
        "JSON",
        "Markdown",
        "ESLint",
        "Prettier",
        "textlint",
        "Dialog component",
        "VS Code",
        "Git",
        "stylelint",
        "Husky",
        "end-to-end"
      ]
    }
  }
